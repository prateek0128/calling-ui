{
  "expo": {
    "name": "SMM Support",
    "slug": "calling-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/App-icon.png",
    "scheme": "callingui",
    "platforms": ["android", "ios", "web"],
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.miiscollp.smmsupport",
      "icon": "src/assets/images/App-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/App-icon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "org.miiscollp.smmsupport",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/ee53504a-5741-44c0-a5cd-80fceca151bc"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee53504a-5741-44c0-a5cd-80fceca151bc"
      }
    },
    "owner": "miiscoorg"
  }
}
